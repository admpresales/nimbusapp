version: '0.2.0'
name: nimbusapp-test
description: Simple docker-app definition for testing nimbusapp
namespace: admpresales

---
version: '3.2'

services:
  web:
    container_name: '${WEB_CONTAINER}'
    image: "httpd:2.4"
    ports:
      - "${PORT}:80"
    environment:
      message: "${MESSAGE}"

---
WEB_CONTAINER: 'nimbusapp-test-web'
PORT: 12345
MESSAGE: ""
